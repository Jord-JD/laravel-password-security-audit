{
  "name": "jord-jd/laravel-password-security-audit",
  "description": "Laravel Password Security Audit",
  "type": "library",
  "require": {
    "laravel/framework": "^5.6||^6.0",
    "jord-jd/php-cli-progress-bar": "^null.0",
    "jord-jd/php-password-cracker": "^null.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "license": "LGPL-3.0-only",
  "authors": [
    {
      "name": "Jordan Hall",
      "email": "jordan@hall05.co.uk"
    }
  ],
  "autoload": {
    "psr-4": {
      "JordJD\\LaravelPasswordSecurityAudit\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JordJD\\LaravelPasswordSecurityAudit\\ServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  },
  "replace": {
    "divineomega/laravel-password-security-audit": "self.version"
  }
}
